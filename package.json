    "tbl_cpi": "npx sequelize-cli model:generate --name tbl_cpi --attributes id_kriteria:integer,id_value:integer,id_order:integer",
    "tbl_notification": "npx sequelize-cli model:generate --name tbl_notification --attributes user_id:integer,message:string,isRead:boolean",
    "tbl_room": "npx sequelize-cli model:generate --name tbl_room --attributes id_ustadz:integer,nameroom:string",
    "tbl_role": "npx sequelize-cli model:generate --name tbl_role --attributes role_name:string",
    "seed-pegawai": "npx sequelize-cli seed:generate --name demo-tbl_pegawai",
    "seed-role": "npx sequelize-cli seed:generate --name demo-tbl_role",
    "seed-santri": "npx sequelize-cli seed:generate --name demo-tbl_santri",
    "seed-room": "npx sequelize-cli seed:generate --name demo-tbl_room",
    "seed-req": "npx sequelize-cli seed:generate --name demo-tbl_req",
    "seed-notification": "npx sequelize-cli seed:generate --name demo-tbl_notification",
    "seed-cpi": "npx sequelize-cli seed:generate --name demo-tbl_cpi",
    "seed-kriteria": "npx sequelize-cli seed:generate --name demo-tbl_kriteria",
    "seed-subkriteria": "npx sequelize-cli seed:generate --name demo-tbl_subkriteria",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo": "npx sequelize-cli db:migrate:undo:all",
    "db:drop": "npx sequelize-cli db:drop"
