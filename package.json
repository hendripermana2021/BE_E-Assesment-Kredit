    "tbl_santri": "npx sequelize-cli model:generate --name tbl_santri --attributes name_santri:string,sex:boolean,fathername:string,mothername:string,id_room:integer,status:boolean,accesstoken:text,refreshtoken:text",
    "tbl_pegawai": "npx sequelize-cli model:generate --name tbl_pegawai --attributes name_pegawai:string,sex:boolean,email:string,password:string,role_id:string,accesstoken:text,refreshtoken:text",
