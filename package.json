{
  "name": "backend-e-permission",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:create": "npx sequelize db:create",
    "dev": "nodemon ./index.js",
    "start": "node ./index.js",
    "tbl_users": "npx sequelize-cli model:generate --name tbl_users --attributes name_user:string,gender:string,email:string,password:string,real_password:string,role_id:integer,image:string,accesstoken:text,refreshtoken:text",
    "tbl_nasabah": "npx sequelize-cli model:generate --name tbl_nasabah --attributes name_nasabah:string,id_user:integer,gender:string,marital_status:string,fathername:string,mothername:string,status:boolean,no_hp:string,place_of_birth:string,birthday:date,address:string,image:string,nik:number,job_title:string,monthly_income:float,employment_status:string,work_address:string,long_work_at_company:integer",
    "tbl_req": "npx sequelize-cli model:generate --name tbl_req --attributes id_nasabah:integer,id_user:integer,created_by:integer,cpi_result:float,commented:string,id_calculated:integer,status_ajuan:string,id_calculated:integer,jlh_dana:float,purpose_req:string",
    "tbl_kriteria": "npx sequelize-cli model:generate --name tbl_kriteria --attributes scale_priority:integer,name_kriteria:string,weight_score:float,type:boolean",
    "tbl_subkriteria": "npx sequelize-cli model:generate --name tbl_subkriteria --attributes name_sub:string,id_kriteria:integer,description:text,value:float",
    "tbl_cpi": "npx sequelize-cli model:generate --name tbl_cpi --attributes id_kriteria:integer,id_subkriteria:integer,id_order:integer",
    "tbl_notification": "npx sequelize-cli model:generate --name tbl_notification --attributes user_id:integer,message:string,isRead:boolean",
    "tbl_document": "npx sequelize-cli model:generate --name tbl_document --attributes id_nasabah:integer,name_document:string,file:string,id_req:integer",
    "tbl_role": "npx sequelize-cli model:generate --name tbl_role --attributes role_name:string",
    "tbl_calculated": "npx sequelize-cli model:generate --name tbl_calculated --attributes created_by:integer",
    "tbl_contact": "npx sequelize-cli model:generate --name tbl_contact --attributes family_relationship:string,name_family:string,no_hp:string,id_nasabah:string",
    "tbl_aset": "npx sequelize-cli model:generate --name tbl_aset --attributes id_nasabah:integer,jenis_aset:string,description_aset:text,nilai_aset:float,kondisi_aset:string,lokasi_aset:string,keterangan:text",
    "tbl_income": "npx sequelize-cli model:generate --name tbl_income --attributes id_nasabah:integer,id_req:integer,sumber_pendapatan:string,jenis_pendapatan:string,nilai_pendapatan:float,durasi_pendapatan:integer,keterangan:text",
    "tbl_document_aset": "npx sequelize-cli model:generate --name tbl_document_aset --attributes id_aset:integer,name_document:string,file:text",
    "tbl_document_ajuan": "npx sequelize-cli model:generate --name tbl_document_ajuan --attributes id_req:integer,name_document:string,file:text",
    "seed-pegawai": "npx sequelize-cli seed:generate --name demo-tbl_pegawai",
    "seed-role": "npx sequelize-cli seed:generate --name demo-tbl_role",
    "seed-santri": "npx sequelize-cli seed:generate --name demo-tbl_santri",
    "seed-calculated": "npx sequelize-cli seed:generate --name demo-tbl_calculated",
    "seed-room": "npx sequelize-cli seed:generate --name demo-tbl_room",
    "seed-req": "npx sequelize-cli seed:generate --name demo-tbl_req",
    "seed-notification": "npx sequelize-cli seed:generate --name demo-tbl_notification",
    "seed-cpi": "npx sequelize-cli seed:generate --name demo-tbl_cpi",
    "seed-kriteria": "npx sequelize-cli seed:generate --name demo-tbl_kriteria",
    "seed-subkriteria": "npx sequelize-cli seed:generate --name demo-tbl_subkriteria",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo": "npx sequelize-cli db:migrate:undo:all",
    "db:drop": "npx sequelize-cli db:drop",
    "db:start": "pnpm db:create && pnpm db:migrate && pnpm db:seed && pnpm dev",
    "db:restart": "pnpm db:drop && pnpm db:create && pnpm db:migrate && pnpm db:seed && pnpm dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-loader": "10.0.0",
    "bcrypt": "5.1.1",
    "body-parser": "2.2.0",
    "cookie-parser": "1.4.7",
    "cookie-session": "2.1.0",
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "express": "5.1.0",
    "form-data": "4.0.2",
    "fs": "0.0.2",
    "js-yaml": "4.1.0",
    "jsonwebtoken": "9.0.2",
    "latest": "0.2.0",
    "morgan": "1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "2.18.1",
    "mysql2": "3.14.0",
    "nodemon": "3.1.9",
    "part": "0.1.1",
    "path": "0.12.7",
    "pg": "8.14.1",
    "sequelize": "6.37.7",
    "sequelize-cli": "6.6.2",
    "socket.io": "4.8.1",
    "swagger": "0.7.5",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1"
  }
}
